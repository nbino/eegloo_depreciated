<!-- start content -->
<div id="fullcontentcenter" align="center">
	<fieldset class="colored"><legend class="colored bigtext">Add Some Details</legend>
	
	<div id="listview_m" style="margin-left:50px">
	
	<ul>
		<% form_for( @listing_info, :url=>listing_listing_info_url(@listing) ) do |f| %>
		
		
		<li class="whole line">Numbers</li>
		<li><label for="listing_info_sq_footage">Sq Footage</label>
		<%=f.text_field :sq_footage, :size => 4 , :class=>'required text' %></li>
		<li><label for="listing_info_no_of_bathrooms"># Bathrooms</label>
		<%= f.select :no_of_bathrooms, {'1'=>'1','2'=>'2','3'=>'3','4'=>'4'},{:include_blank=>'[select]', :class=>'required'} %></li>
		<li><label for="listing_info_ceiling_height">Ceiling Height</label>
		<%=f.text_field :ceiling_height, :size => 4 , :class=>'required text'%></li>	<li><label for="listing_info_landlord_phone_number">Landlord #</label>
		<%=f.text_field :landlord_phone_number, :size => 10 , :class=>'required texts' %></li>

		
		<li class="whole line">Essentials</li>
		<li><label for="listing_info_maintenance_q">Maintenance</label>
		<%= f.collection_select  :maintenance_q_id, @maintenance_select, :id, :name, {:include_blank=>'[select]', :class=>'required'} %></li>
		<li><label for="listing_info_appliance_q">Appliance Cond</label>
		<%= f.collection_select  :appliances_q_id, @appliances_select, :id, :name, {:include_blank=>'[select]', :class=>'required'} %></li>
		<li><label for="listing_info_bathroom_q">Bathroom Cond</label>   
		<%= f.collection_select  :bathroom_q_id, @bathroom_select, :id, :name, {:include_blank=>'[select]', :class=>'required'} %></li>	

	
		<li class="whole line">Primary</li>
		
		<li><label for="listing_info_elevator">Elevator</label><%= f.yes_no_radio :elevator %></li>
		<li><label for="listing_info_laundry">Laundry</label><%= f.yes_no_radio :laundry %></li>
		<li><label for="listing_info_gym">Gym</label><%= f.yes_no_radio :gym %></li>
		<li><label for="listing_info_doorman">Doorman</label><%= f.yes_no_radio :doorman %></li>
		<li><label for="listing_info_pets">Pets</label> 
		<%= f.collection_select  :pets_id, @pets_choices, :id, :name, {:include_blank=>'[select]', :class=>'required'} %></li>
		<li><label for="listing_info_heat_q">Heat</label>   
		<%= f.collection_select  :heat_q_id, @heat_select, :id, :name, {:include_blank=>'[select]', :class=>'required'} %></li>
		<li><label for="listing_info_heat_q">A/C</label><%= f.collection_select  :ac_type, @ac_types, :id, :name, {:include_blank=>'[select]', :class=>'required'} %></li>
		
		<li class="whole line">Secondary</li>
		
		<li><label for="listing_info_street_noise_level">Street Noise</label> 
		<%= f.collection_select  :street_noise_level_id, @street_noise_levels, :id, :name, {:include_blank=>'[select]', :class=>'required'} %></li>
		<li><label for="listing_info_street_noise_level">Neighbor Noise</label> 
		<%= f.collection_select  :nbors_noise_level_id, @nbors_noise_levels, :id, :name, {:include_blank=>'[select]', :class=>'required'} %></li>
		
		<li><label for="listing_info_convertable">Convertible</label><%= f.yes_no_radio :convertable%></li> 
		<li><label for="listing_info_multi_level">Multi Level</label><%= f.yes_no_radio :multi_level %></li>
		<li><label for="listing_info_penthouse">Penthouse</label><%= f.yes_no_radio :penthouse %></li>
		<li><label for="listing_info_patio">Patio</label><%= f.yes_no_radio :patio %></li>
		<li><label for="listing_info_balcony">Balcony</label><%= f.yes_no_radio :balcony %></li>
		<li><label for="listing_info_separate_kitchen">Seperate Kitchen</label><%= f.yes_no_radio :separate_kitchen%></li> 
		<li><label for="listing_info_back_yard">Backyard</label>   
		<%= f.collection_select  :back_yard_type_id, @back_yard_types, :id, :name, {:include_blank=>'[select]', :class=>'required'} %></li>
		<li><label for="listing_info_roof_access">Roof Access</label>   
		<%= f.collection_select  :roof_access_type_id, @roof_access_types, :id, :name, {:include_blank=>'[select]', :class=>'required'} %></li>
		<li><label for="listing_info_floor_type">Floor Type</label><%= f.collection_select  :floor_type, @floor_types, :id, :name, {:include_blank=>'[select]', :class=>'required'} %></li>
		<li><label for="listing_info_cellphone">Cellphone</label> 
		<%= f.collection_select  :cellphone_q_id, @cellphone_select, :id, :name, {:include_blank=>'[select]', :class=>'required'} %></li>
		<li><label for="listing_info_broadband">Broadband</label><%= f.yes_no_radio :broadband %></li>	
		<li><label for="listing_info_roaches">Roaches</label><%= f.yes_no_radio :roaches %></li>	
		<li><label for="listing_info_rodents">Rodents</label><%= f.yes_no_radio :rodents %></li>		
		<li><label for="listing_info_rent_controlled">Rent Controlled</label><%= f.yes_no_radio :rent_controlled%></li>
		<li><label for="listing_info_rent_stabilized">Rent Stabilized</label><%= f.yes_no_radio :rent_stabilized%></li> 

		
		
		<li class="whole">
		<%= render :partial => '/shared/submit_button' %>
		</li>
		<% end %>
	</ul>
	
	</div>
	</fieldset>
</div>

